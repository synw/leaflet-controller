{
  "name": "leaflet-controller",
  "version": "0.1.0",
  "description": "A stateful map controller for Leaflet",
  "repository": "https://github.com/synw/leaflet-controller",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "docs": "typedoc --entryPointStrategy expand"
  },
  "peerDependencies": {
    "leaflet": "^1.9.4"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.8",
    "@types/node": "^20.10.4",
    "rollup-plugin-typescript2": "^0.36.0",
    "typedoc": "^0.25.4",
    "typedoc-plugin-markdown": "^3.17.1",
    "typedoc-plugin-rename-defaults": "^0.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.7"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/leaflet-controller.umd.js",
  "module": "./dist/leaflet-controller.es.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/leaflet-controller.es.js",
      "require": "./dist/leaflet-controller.umd.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT"
}